{
  "name": "west-point-tide-widget",
  "version": "0.0.4",
  "description": "A little web app that tells you the weather conditions in Ballard, Seattle",
  "main": "index.js",
  "scripts": {
    "deploy": "sls deploy",
    "deploy:client": "serverless client deploy --region us-west-2 && aws cloudfront create-invalidation --distribution-id E13JOGYJXQZL0I --paths \"/*\"",
    "client": "live-server --host=localhost --port=3001 client/dist",
    "client:dev": "cd client && webpack --mode development",
    "lint": "eslint client/**/*.js src/**/*.js",
    "lint:fix": "eslint client/**/*.js src/**/*.js --fix",
    "eslint-check": "npx eslint-config-prettier .eslintrc.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "serverless offline start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivanoats/SeattleTide.git"
  },
  "keywords": [
    "weather",
    "serverless",
    "seattle"
  ],
  "author": "Ivan Storck",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ivanoats/SeattleTide/issues"
  },
  "homepage": "https://github.com/ivanoats/SeattleTide#readme",
  "dependencies": {
    "@babel/core": "^7.15.5",
    "babel-loader": "^8.2.2",
    "express": "^4.17.1",
    "lodash": "^4.17.21",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "serverless-finch": "^2.6.0",
    "serverless-http": "^2.7.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-json-strings": "^7.14.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/preset-env": "^7.15.6",
    "ajv": "^8.6.3",
    "csv-parse": "^4.16.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-prettier-standard": "^4.0.1",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "got": "^11.8.2",
    "live-server": "^1.2.1",
    "prettier": "^2.4.0",
    "prettier-config-standard": "^4.0.0",
    "serve": "^12.0.1",
    "serverless-domain-manager": "^5.1.5",
    "serverless-offline": "^8.1.0",
    "serverless-webpack": "^5.5.4",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.8.0"
  }
}
